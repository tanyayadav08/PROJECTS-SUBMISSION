package javaprograms;

public class ImplicitTypeCasting {

	public static void main(String[] args) {
		
		int a = 10;
		
		double b;
		b = a;
		
		System.out.println(b);

	}

}



package javaprograms;

public class ExplicitTypeCasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double a = 11.34;
		
		int b = (int)a;
		
		System.out.println(b);

	}

}
